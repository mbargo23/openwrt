/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9344.dtsi"

/ {
	compatible = "mikrotik,routerboard-rb2011-uias-2hnd", "qca,ar9344";
	model = "Mikrotik RouterBoard RB2011UiAS-2HnD";

	chosen {
		bootargs = "lcd_ctrl=84 parts=1 boot_part_size=4194304 gpio=249403 HZ=375000000 mem=128M kmac=CC:2D:E0:62:70:D6 board=2011r5 board=2011r5 ver=3.41 hw_opt=00718004 boot=1 mlc=11 console=ttyS0,115200 ubi.mtd=2,2048 rootfstype=squashfs noinitrd";
	};

	aliases {
		serial0 = &uart;
	};

	leds {
		compatible = "gpio-leds";

		power {
			label = "mikrotik:green:power";
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	gpio-export {
		compatible = "gpio-export";

		gpio_nand_power {
			gpio-export,name = "rb2011:power:nand";
			gpio-export,output = <0>;
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};
	};

	beeper {
		compatible = "gpio-beeper";
		gpios = <&gpio 19 GPIO_ACTIVE_HIGH>;
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "routerboot";
				reg = <0x0 0x0>;
				read-only;
				compatible = "mikrotik,routerboot-partitions";
				#address-cells = <1>;
				#size-cells = <1>;

				hard_config: hard_config {
					read-only;
				};

				bios {
					size = <0x1000>;
					read-only;
				};

				soft_config {
				};
			};
		};
	};
};

&nand {
	status = "okay";

	nand-ecc-mode = "soft";
	qca,nand-swap-dma;
	qca,nand-scan-fixup;

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "booter";
			reg = <0x0000000 0x0040000>;
			read-only;
		};

		partition@40000 {
			label = "kernel";
			reg = <0x0040000 0x03c0000>;
		};

		partition@400000 {
			label = "ubi";
			reg = <0x0400000 0x7c00000>;
		};
	};
};

&uart {
	status = "okay";
};

&eth0 {
	status = "okay";

	phy-mode = "rgmii";
	phy-handle = <&gswphy0>;

};


&eth1 {
	status = "okay";

	phy-mode = "rgmii";
	phy-handle = <&swphy0>;

	gmac-config {
		device = <&gmac>;
		switch-phy-swap = <0>;
	};

};

&mdio0 {
        status = "okay";

        phy-mask = <0>;

        gswphy0: ethernet-phy@0 {
		compatible = "qca,ar8327";
                reg = <0>;
		phy-mode = "rgmii";
                qca,mib-poll-interval = <500>;
                //reset-gpios = <&gpio 11 GPIO_ACTIVE_LOW>;

                qca,ar8327-initvals = <
			0x04 0x07600000 /* PORT0 PAD MODE CTRL */
			0x08 0x01000000 /* PORT5 PADÂ MODE CTRL*/
			0x0c 0x00000080 /* PORT6 PAD MODE CTRL */
			/* 0x10 0x40000000 /* Power-on Strapping: 176-pin interface configuration */
			0x10 0x81000080 /* POWER_ON_STRIP */
			0x50 0xc437c437 /* LED Control Register 0 */
			0x54 0xc337c337 /* LED Control Register 1 */
			0x58 0x00000000 /* LED Control Register 2 */
			0x5c 0x03ffff00 /* LED Control Register 3 */
			0x7c 0x0000004e /* PORT0_STATUS */
			0x94 0x0000004e /* PORT6 STATUS */
			0xe0 0xc74164de /* SGMII_CTRL */
			0xe4 0x000aa545 /* MAC_POWER_SEL */
			>;
		};

};

